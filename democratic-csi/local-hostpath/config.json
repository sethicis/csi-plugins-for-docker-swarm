{
    "description": "democratic-csi storage driver for synology iscsi",
    "interface": {
      "types": ["docker.csinode/1.0", "docker.csicontroller/1.0"],
      "socket": "csi-synology-iscsi.sock"
    },
    "network": {
      "type": "host"
    },
    "mounts": [
      {
        "name": "csi-synology-iscsi",
        "description": "synology iscsi path",
        "type": "bind",
        "source": "/var/lib/csi-synology-iscsi",
        "destination": "/var/lib/csi-synology-iscsi",
        "options": ["bind"],
        "settable": ["source"]
      }
    ],
    "env": [
      {
        "name": "CSI_ENDPOINT",
        "value": "/run/docker/plugins/csi-synology-iscsi.sock"
      }
    ],
    "entrypoint": [
      "/home/csi/app/entrypoint.sh"
    ],
    "workdir": "/home/csi/app",
    "linux": {
      "capabilities": [
        "CAP_SYS_ADMIN"
      ],
      "AllowAllDevices": true,
      "devices": null
    },
    "PropagatedMount": "/data/published"
  }